{
  "name": "better-discord-chess-bot",
  "version": "0.0.1",
  "description": "A better chess bot",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "cd dist && concurrently \"tsc -w\" \"nodemon dist/index.js\" && cd ..",
    "start": "cd dist && node index.js && cd ..",
    "dev": "npm run build && npm run start"
  },
  "keywords": [
    "chess",
    "bot",
    "discord.js"
  ],
  "author": "FirstPotatoMan",
  "license": "MIT",
  "dependencies": {
    "chess.js": "^0.11.0",
    "discord.js": "^12.3.1"
  },
  "devDependencies": {
    "@types/chess.js": "^0.10.1",
    "@types/node": "^14.11.3",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4",
    "typescript": "^4.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FirstPotatoMan/Better-Discord-Chess-Bot.git"
  },
  "bugs": {
    "url": "https://github.com/FirstPotatoMan/Better-Discord-Chess-Bot/issues"
  },
  "homepage": "https://github.com/FirstPotatoMan/Better-Discord-Chess-Bot#readme"
}
